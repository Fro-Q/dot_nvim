{
  "define snippet": {
    "prefix": "snip",
    "body": [
      "\"${1:name}\": {",
      "\t\"prefix\": \"${2:prefix}\",",
      "\t\"body\": [",
      "\t\t\"${3:body}\"",
      "\t]",
      "}"
    ]
  },
  "add snippet lang": {
    "prefix": "lang",
    "body": [
      "{",
      "\t\"language\": [\"${1:language}\"],",
      "\t\"path\": \"${2:path}\",",
      "}"
    ]
  }
}
